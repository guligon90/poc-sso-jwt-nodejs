{
	"root": true,
	"plugins": ["prettier", "jest"],
	"extends": [
		"eslint:recommended",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-console": 1,
		"no-unused-vars": 1,
		"prettier/prettier": 2
	},
	"env": {
		"browser": true,
		"node": true,
		"jest/globals": true,
		"es6": true
	}
}
